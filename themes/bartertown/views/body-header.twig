{% if menu_main %}
<nav id="site-navigation" role="navigation" class="navbar">
    <a href="#content" class="sr-only">Skip to Content</a>
    <div class="container">
        <ul class="top-level">
{% for item in menu_main.get_items %}
            <li class="dropdown{% if item.children.0.title == 'bubble' %} bubble{% endif %}">
                {% if item.url == '#' %}<span class="dropdown-toggle">{{item.title}}{% if item.children %} <b class="caret"></b>{% endif %}</span>
                {% else %}<a href="{{item.url}}" class="dropdown-toggle">{{item.title}}{% if item.children %} <b class="caret"></b>{% endif %}</a>
                {% endif %}
{% if item.children %}
                <ul class="dropdown-menu">
{# We have two types of drop-downs: Single-column and bubble-with-ad-and-recent-stories. #}
{# If we're doing the bubble nav, the first item in the child nav will be named "bubble", #}
{# and if it has a link the link will be an RSS feed for us to ingest and publish. #}
{# No RSS feed == use the articles from the parent category. #}
{% if item.children.0.title != 'bubble' %}
{% for subitem in item.children %}
            <li><a href="{{subitem.url}}">{{subitem.title}}</a></li>
{% endfor %}
{% else %}
                    <li class="bubble-content">
  <ul class="pager">
    <li class="prev"><a href="#">&#8592; Close</a></li>
    <li class="next"><a href="#">More topics &#8594;</a></li>
  </ul>
  <div class="bubble-editorial-content">
    <div class="bubble-overflow">
      <div class="bubble-subtopics">
        <div class="visible-pane">
          <ul class="pane-container">
{% for subitem in item.children %}
{% if subitem.title != 'bubble' %}
            <li class="topic-pane"><a href="{{subitem.url}}">{{subitem.title}}</a></li>
{% endif %}
{% endfor %}
          </ul>
        </div>
      </div>
    {# We only publish the recent headlines in the bubble if the parent menu #}
    {# item is a category. #}
    {% if item.object == 'category' %}
      <div class="bubble-headlines">
        {% for post in TimberTerm(item.name).get_posts('numberposts=4') %}
            {% if loop.first %}
                {% if post.thumbnail.src %}
        <a href="{{post.guid}}"><img src="{{post.thumbnail.src}}" width="120" alt="" /></a>
                {% endif %}
        <h3><a href="{{post.guid}}">{{post.title}}</a></h3>
        <p>{{post.excerpt}}</p>
        <div class="link-list">
          <ul>
            {% else %}
            <li><a href="{{post.guid}}">{{post.title}}</a></li>
            {% endif %}
            {% if loop.last %}
          </ul>
        </div> <!-- .link-list -->
            {% endif %}
        {% endfor %}
      </div>
    {% endif %}
    </div>
  </div>

    <div class="ad medium-rectangle visible-lg-up">
        {# Still need to find out how these are going to be targeted. #}
        <img src="http://placehold.it/300x250&amp;text=300x250+navad" alt="300x250 navad">
    </div>
{% endif %}
                    </li>
                </ul>
{% endif %}
            </li>
{% endfor %}
        </ul>
    </div>
</nav>
{% endif %}

{% if menu_hot %}
<nav id="hot-topics">
    <div class="container">
        <ul class="list-inline">
            <li>
                Hot Topics:
            </li>
{% for item in menu_hot.get_items %}
            <li id="menu-item-{{item.ID}}" class="menu-item menu-item-type-{{item.type}} menu-item-object-{{item.object}} menu-item-{{item.ID}}">
                <a href="{{ item.get_path }}" title="{{ item.title }}">{{ item.title }}</a>
            </li>
{% endfor %}
                    </ul>
    </div> <!-- .container -->
</nav>
{% endif %}
