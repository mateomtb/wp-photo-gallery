        </xs:choice>
    </xs:complexType>
   <!-- Access Level -->
   <xs:element name="accesslevel" type="sax:AccessLevelType" />
    <xs:complexType name="AccessLevelType">
     <xs:sequence>
           <xs:element name="name" type="xs:string" maxOccurs="1" minOccurs="0" />
         <xs:element name="sortOrder" type="xs:string" maxOccurs="1" minOccurs="0" />
        </xs:sequence>
      <xs:attribute name="id" type="xs:long" use="required"></xs:attribute>
       <xs:attribute name="uri" type="xs:anyURI" use="required"></xs:attribute>
    </xs:complexType>
