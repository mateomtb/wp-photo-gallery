        <xs:choice minOccurs="0" maxOccurs="unbounded">
         <xs:element name="category" type="sax:CategoryListItemType" nillable="true" />
      </xs:choice>
    </xs:complexType>
   <!-- Publication -->
    <xs:element name="publication" type="sax:PublicationType" nillable="true" />
    <xs:complexType name="PublicationType">
     <xs:sequence>
           <xs:element name="name" type="xs:string" maxOccurs="1" minOccurs="0" nillable="true" />
