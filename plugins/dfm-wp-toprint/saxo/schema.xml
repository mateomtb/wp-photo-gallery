       <xs:documentation xml:lang="en">
        The Editorial Web Services XML Schema. Defines types, lists, etc.       </xs:documentation>
 </xs:annotation>
    <xs:complexType name="BaseItemType">
        <xs:sequence>
           <xs:element name="createdtime" type="xs:dateTime" maxOccurs="1" minOccurs="0" />
            <xs:element name="createdby" type="sax:InternalResourceType" maxOccurs="1" minOccurs="0" />
         <xs:element name="updatedtime" type="xs:dateTime" maxOccurs="1" minOccurs="0" />
            <xs:element name="updatedby" type="sax:InternalResourceType" maxOccurs="1" minOccurs="0" />
         <xs:element name="site" type="sax:SiteType" />
          <xs:element name="status" type="sax:StatusType" maxOccurs="1" minOccurs="0" />
          <xs:element name="category" type="sax:CategoryType" maxOccurs="1" minOccurs="0" />
          <xs:element name="publication" type="sax:PublicationType" maxOccurs="1" minOccurs="0" />
