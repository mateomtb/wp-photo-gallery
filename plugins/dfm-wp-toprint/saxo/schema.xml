                    <xs:element name="assigned" type="xs:boolean" maxOccurs="1" minOccurs="0" nillable="true" />
                    <xs:element name="linkcount" type="xs:long" maxOccurs="1" minOccurs="0" nillable="true" />
                    <xs:element name="planning" type="sax:PlanElementListType" maxOccurs="1" minOccurs="0" nillable="true" />
