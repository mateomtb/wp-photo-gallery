   <xs:complexType name="AccessLevelListType">
     <xs:choice minOccurs="0" maxOccurs="unbounded">
         <xs:element name="accesslevel" type="sax:AccessLevelType" />
        </xs:choice>
    </xs:complexType>
   <!-- Article aka Story -->
  <xs:element name="story" type="sax:ArticleListItemType" />
  <xs:complexType name="ParagraphListType">
       <xs:sequence>
           <xs:element name="paragraph" type="xs:string" maxOccurs="unbounded" minOccurs="0" />
        </xs:sequence>
  </xs:complexType>
   <xs:complexType name="NativeBoxChunkListType">
      <xs:sequence>
           <xs:element name="chunk" type="xs:string" maxOccurs="unbounded" minOccurs="0" />
        </xs:sequence>
  </xs:complexType>
   <!-- Element can have either text or chunk sub-elements -->
 <xs:complexType name="NativeBoxTextType">
       <xs:choice>
            <xs:element name="xhtml" type="xs:string" maxOccurs="1" minOccurs="0" />
            <!-- 20130606 WJU Read/Write SMS & NetHeader as ICML (FUSDEV-245) -->
            <xs:element name="plaintext" type="xs:string" maxOccurs="1" minOccurs="0" />
            <xs:element name="chunk" type="xs:string" maxOccurs="unbounded" minOccurs="0" />
        </xs:choice>
        <xs:attribute name="boxtype" type="xs:string" use="required" />
     <xs:attribute name="nativetexttype" type="xs:string" use="required" />
      <xs:attribute name="encoding" type="xs:string" use="required" />
    </xs:complexType>
   <!-- Element can have either text or chunk sub-elements -->
 <xs:complexType name="NativeBoxTextListType">
       <xs:sequence>
           <xs:element name="nativeboxtext" type="sax:NativeBoxTextType" maxOccurs="unbounded" minOccurs="0" />
        </xs:sequence>
  </xs:complexType>
   <xs:complexType name="TextElementListType">
     <xs:sequence>
           <xs:element name="textelementtype" type="xs:string" maxOccurs="unbounded" minOccurs="0" />
      </xs:sequence>
  </xs:complexType>
   <xs:complexType name="BoxListType">
     <xs:sequence>
           <xs:element name="box" type="xs:string" maxOccurs="unbounded" minOccurs="0" />
      </xs:sequence>
  </xs:complexType>
     <!-- 20130610 WJU Read/Write SMS & NetHeader as ICML (FUSDEV-245) -->
 <xs:complexType name="PlainTextType">
        <xs:sequence>
            <xs:element name="plaintext" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="chunk" type="xs:string" maxOccurs="unbounded" minOccurs="0" />
        </xs:sequence>
        <xs:attribute name="boxtype" type="xs:string" use="required" />
        <xs:attribute name="nativetexttype" type="xs:string" use="required" />
        <xs:attribute name="encoding" type="xs:string" use="required" />
   </xs:complexType>
    <xs:complexType name="PlainTextListType">
        <xs:sequence>
            <xs:element name="plaintext" type="xs:string" maxOccurs="unbounded" minOccurs="0" />
        </xs:sequence>
        <xs:attribute name="type" type="xs:string" use="required" />
    </xs:complexType>
  <xs:complexType name="NetHeaderListType">
        <xs:complexContent>
            <xs:extension base="sax:PlainTextType" />
        </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="SmsListType">
        <xs:complexContent>
            <xs:extension base="sax:PlainTextType" />
        </xs:complexContent>
 </xs:complexType>
