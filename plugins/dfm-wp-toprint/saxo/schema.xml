        <xs:simpleContent>
          <xs:extension base="xs:string">
             <xs:attribute name="uri" type="xs:anyURI" use="optional"></xs:attribute>
                <xs:attribute name="site" type="xs:long" use="required" />
              <xs:attribute name="id" type="xs:long" use="required" />
                <xs:attribute name="type" type="xs:long" use="optional" />
          </xs:extension>
     </xs:simpleContent>
 </xs:complexType>
   <xs:element name="categories" type="sax:CategoryListType" />
